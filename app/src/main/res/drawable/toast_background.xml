<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/buttonCornerRadius" />
            <solid android:color="@color/colorPrimary" />
            <padding android:bottom="@dimen/buttonPadding" android:left="@dimen/buttonPadding" android:right="@dimen/buttonPadding" android:top="@dimen/buttonPadding" />
            <stroke android:width="@dimen/buttonStrokeWidth" android:color="@color/black" />
        </shape>
    </item>


</selector>